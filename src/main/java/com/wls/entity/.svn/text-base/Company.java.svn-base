package com.wls.entity;
/***
 * 公司类
 * 存放公司的基本信息
 * @author long
 *
 */
public class Company {
	//公司的id
	private int c_id;
	//创建公司的用户的邮箱
	private String email;
	//公司名
	private String c_name;
	//公司地址
	private String c_place;
	//公司类型
	private String c_type;
	//公司的融资阶段
	private String c_level;
	//公司的logo
	private String c_imag;
	//对公司的评论
	private int c_comment;
	//对公司的描述
	private String c_desc;
	public final int getC_id() {
		return c_id;
	}
	public final void setC_id(int c_id) {
		this.c_id = c_id;
	}
	public final String getEmail() {
		return email;
	}
	public final void setEmail(String email) {
		this.email = email;
	}
	public final String getC_name() {
		return c_name;
	}
	public final void setC_name(String c_name) {
		this.c_name = c_name;
	}
	public final String getC_place() {
		return c_place;
	}
	public final void setC_place(String c_place) {
		this.c_place = c_place;
	}
	public final String getC_type() {
		return c_type;
	}
	public final void setC_type(String c_type) {
		this.c_type = c_type;
	}
	public final String getC_level() {
		return c_level;
	}
	public final void setC_level(String c_level) {
		this.c_level = c_level;
	}
	public final String getC_imag() {
		return c_imag;
	}
	public final void setC_imag(String c_imag) {
		this.c_imag = c_imag;
	}
	public final int getC_comment() {
		return c_comment;
	}
	public final void setC_comment(int c_comment) {
		this.c_comment = c_comment;
	}
	public final String getC_desc() {
		return c_desc;
	}
	public final void setC_desc(String c_desc) {
		this.c_desc = c_desc;
	}
	public Company(String email, String c_name, String c_place, String c_type, String c_level, String c_desc) {
		super();
		this.email = email;
		this.c_name = c_name;
		this.c_place = c_place;
		this.c_type = c_type;
		this.c_level = c_level;
		this.c_desc = c_desc;
	}
	@Override
	public String toString() {
		return "Company [c_id=" + c_id + ", email=" + email + ", c_name=" + c_name + ", c_place=" + c_place + ", c_type=" + c_type + ", c_level=" + c_level + ", c_imag=" + c_imag + ", c_comment=" + c_comment + ", c_desc=" + c_desc + "]";
	}
	

	public Company() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Company(int c_id,String c_name, String c_place, String c_type,
			String c_level,  String c_desc) {
		super();
		this.c_id=c_id;
		this.c_name = c_name;
		this.c_place = c_place;
		this.c_type = c_type;
		this.c_level = c_level;
		this.c_desc = c_desc;
	}

}
